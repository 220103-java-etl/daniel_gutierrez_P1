<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>NewUserServlet</servlet-name>
        <servlet-class>com.revature.servlets.NewUserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ReimbursementServlet</servlet-name>
        <servlet-class>com.revature.servlets.ReimbursementServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newReimbursementServlet</servlet-name>
        <servlet-class>com.revature.servlets.newReimbursementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewUserServlet</servlet-name>
        <url-pattern>/n</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReimbursementServlet</servlet-name>
        <url-pattern>/r</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newReimbursementServlet</servlet-name>
        <url-pattern>/newR</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>